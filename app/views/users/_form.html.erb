

<%= form_for(user, url: destination_url) do |f| %>
  <%= f.text_field :name, placeholder: 'Your First & Last Name'  %>
  <%= f.text_field :email, placeholder: 'Your Email'  %>
  <%= f.password_field :password, placeholder: 'Password'  %>
  <%= f.submit "Register", :class => 'button'%>
<% end %>


